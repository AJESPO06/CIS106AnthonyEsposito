    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="This program calculates the Weekly, Monthly, and Annual Gross Pay"/>
            <declare name="hoursWorked" type="Integer" array="False" size=""/>
            <declare name="hourlyWage" type="Integer" array="False" size=""/>
            <declare name="weeklyPay" type="Integer" array="False" size=""/>
            <declare name="monthlyPay" type="Integer" array="False" size=""/>
            <declare name="annualPay" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter hours worked&quot;" newline="False"/>
            <input variable="hoursWorked"/>
            <output expression="&quot;Enter hourly wage&quot;" newline="True"/>
            <input variable="hourlyWage"/>
            <assign variable="weeklyPay" expression="hourlyWage * hoursWorked"/>
            <assign variable="monthlyPay" expression="(hourlyWage * hoursWorked) * 4"/>
            <assign variable="annualPay" expression="(hourlyWage * hoursWorked) * 52"/>
            <output expression="&quot;Weekly pay is &quot; &amp; weeklyPay" newline="False"/>
            <output expression="&quot; Monthly pay is &quot; &amp; monthlyPay" newline="True"/>
            <output expression="&quot;Annual pay is &quot; &amp; annualPay" newline="True"/>
        </body>
    </function>
</flowgorithm>
